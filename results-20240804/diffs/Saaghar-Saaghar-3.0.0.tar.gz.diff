> bool Bookmarks::write(QIODevice* device)
> {
>     const int IndentSize = 4;
> 
>     QTextStream out(device);
>     out.setCodec("utf-8");
>     QString domString = m_domDocument.toString(IndentSize);
>     domString = domString.replace("&#xd;", "");
>     out << domString;
>     return true;
> }
