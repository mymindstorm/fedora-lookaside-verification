> void DatabaseBrowser::removeThreadsConnections(QObject* obj)
> {
>     QThread* thread = qobject_cast<QThread*>(obj);
> 
>     if (thread) {
>         QStringList connections = s_threadConnections.values(thread);
> 
>         foreach (const QString &connection, connections) {
