> QVariant ConcurrentTask::checkForUpdates()
> {
>     const QString checkByUser = VAR_GET(m_options, checkByUser).toBool()
>                                 ? QLatin1String("CHECK_BY_USER=TRUE") : QLatin1String("CHECK_BY_USER=FALSE");
